{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "camelcase": "off",
    "class-methods-use-this": "off",
    "no-console": "off",
    "quotes": [
      "error",
      "double"
    ],
    "no-return-assign": "off",
    "no-param-reassign": "off",
    "consistent-return": "off"
  }
}
